<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="GanttView">
        <div>
            <div class="oe_gantt_buttons_days">
                <button class="oe_button oe_gantt_button_create">Create</button>
                <button class="oe_button previous_week" type="button">Previous week</button>
                <button class="oe_button previous_day" type="button">Previous day</button>
                <button class="oe_button today" type="button">Today</button>
                <button class="oe_button next_day" type="button">Next day</button>
                <button class="oe_button next_week" type="button">Next week</button>
                <input class="oe_button gantt_improvement_input_date" id="gantt_improvement_input_date" type="date" />
                <button class="oe_button go_date" type="button">Go</button>
            </div>
            <div t-att-id="widget.chart_id" class="oe_gantt"/>
        </div>
    </t>
    <!--<t t-name="GanttView-create-button">
        
    </t>-->
</templates>

